{
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "no-console": 0,
    "global-require": 0,
    "import/no-dynamic-require": 0,
    "import/prefer-default-export": 0,
    "consistent-return": 0,
    "class-methods-use-this": 0,
    "no-restricted-syntax": 0,
    "no-underscore-dangle": 0
  }
}